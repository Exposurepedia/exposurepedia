{
  "name": "boilerplate-s2022",
  "version": "1.0.0",
  "repository": "https://github.com/hack4impact-upenn/boilerplate-s2022.git",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && concurrently \"yarn --cwd src/client install\" \"yarn --cwd src/server install\"",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn server\" \"yarn client\"",
    "server": "cd src/server && cross-env NODE_ENV=development ts-node-dev --respawn index.ts",
    "client": "cd src/client && yarn start"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "prettier": "^2.5.1"
  }
}
