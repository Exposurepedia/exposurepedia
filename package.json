{
  "name": "boilerplate-s2022",
  "version": "1.0.0",
  "repository": "https://github.com/hack4impact-upenn/boilerplate-s2022.git",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "setup": "yarn install",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "server": "cd server && yarn start",
    "postinstall": "echo \"Package install attempted in global directory\"",
    "client": "cd client && yarn start"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "prettier": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ts-node-dev": "^1.1.8"
  }
}
